@model IEnumerable<E_LearningApp.Models.User>
@{
    Layout = "_Layout";
    ViewData["Title"] = "Home Page";
}

<div class="row">
    <div class="col-lg-12">
        <table class="table table-bordered">
            <thead>
                <tr class="bg-primary">
                    <th>Users</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var u in Model)
                {
                    <tr>
                        <td>@u.Email</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>